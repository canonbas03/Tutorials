@model IEnumerable<MVCTutorial.Models.Employee>

<h1>Employees</h1>
<p>
    <a asp-action="Create" class="btn btn-success">Create New Employee</a>
</p>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Salary</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var emp in Model)
        {
            <tr>
                <td>@emp.Id</td>
                <td>@emp.Name</td>
                <td>@emp.Salary€</td>
            </tr>
        }
    </tbody>
</table>
